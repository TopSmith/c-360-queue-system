# Redis connection
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Comma-separated list of queue names
TASK_QUEUES=task_queue_1,task_queue_2

# The callback URL that the remote server will use to notify task status
CALLBACK_URL=http://localhost:8000/callback

# The remote server endpoint to which the worker sends tasks
REMOTE_ENDPOINT=https://remote-server.example.com/start-task